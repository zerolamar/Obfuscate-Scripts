local Section = Tabs.Autofarm:AddSection("Autofarm Bonds")

local Toggle = Tabs.Autofarm:AddToggle("MyToggle", 
{
    Title = "Collect Bonds", 
    Description = "If script automatic leaves for you then its having problems. ( Beta )",
    Default = false,
    Callback = function(state)
	if state then
        loadstring(game:HttpGet("https://pastebin.com/raw/gE2dYCNr"))()
loadstring(game:HttpGet("https://pastebin.com/raw/bbhiGgxs"))()
	else
	    Fluent:Notify({
        Title = "Error",
        Content = "Can not turn off | Autofarm Bonds |",
        Duration = 9
    })
        end
    end 
})

local Toggle = Tabs.Autofarm:AddToggle("Toggle", 
{
    Title = "Win Game", 
    Description = "Wait till 10mins before clicking ( Recommended )",
    Default = false,
    Callback = function(state)
	if state then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-424.448975, 28.055481, -49040.6562, -1, 0, 0, 0, 1, 0, 0, 0, -1)
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local rootPart = character:WaitForChild("HumanoidRootPart")

-- Freeze
rootPart.Anchored = true
humanoid.WalkSpeed = 0
humanoid.JumpPower = 0

wait(10)

-- Unfreeze
rootPart.Anchored = false
humanoid.WalkSpeed = 16 
humanoid.JumpPower = 50
wait(0.5)
game.Players.LocalPlayer.Character.Humanoid.Jump = true
wait(1)
local ts = game:GetService("TweenService")

local teleport = ts:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, TweenInfo.new(6), {CFrame = CFrame.new(-350.900696, 2.99993873, -49040.918, 0.998121321, -2.62078093e-08, 0.061268609, 2.48097507e-08, 1, 2.35792417e-08, -0.061268609, -2.20148859e-08, 0.998121321)})
teleport:Play()
	else
	    Fluent:Notify({
        Title = "Error",
        Content = "Can not turn off.",
        Duration = 9
    })
        end
    end 
})

